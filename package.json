{
  "name": "@kronos-integration/interceptor-line-header",
  "version": "0.0.0-semantic-release",
  "sideEffects": false,
  "type": "module",
  "main": "./src/module.mjs",
  "exports": {
    ".": "./src/module.mjs",
    "./": "./"
  },
  "module": "{{module}}",
  "description": "Splits a line by a sparator into tokens.",
  "keywords": [
    "kronos-interceptor",
    "stream",
    "line",
    "parser",
    "npm-package-template"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "prepare": "rollup -c",
    "pretest": "rollup -c",
    "test": "mocha tests/*-test.js && ava --timeout 2m",
    "posttest": "markdown-doctest",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/module.mjs --section=API",
    "lint": "documentation lint ./src/module.mjs",
    "precover": "rollup -c"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^9.2.0",
    "z-schema": "4.2.3"
  },
  "devDependencies": {
    "ava": "^3.8.2",
    "c8": "^7.1.2",
    "chai": "^4.2.0",
    "documentation": "^13.0.0",
    "jsdoc-to-markdown": "^5.0.2",
    "kronos-test-interceptor": "3.1.1",
    "markdown-doctest": "^1.0.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "rollup": "^2.10.0",
    "semantic-release": "^17.0.7",
    "underscore": "1.9.1"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-line-header.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-line-header/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-line-header#readme",
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "inheritFrom": [
      "arlac77/template-kronos-module",
      "Kronos-Tools/npm-package-template"
    ]
  }
}
